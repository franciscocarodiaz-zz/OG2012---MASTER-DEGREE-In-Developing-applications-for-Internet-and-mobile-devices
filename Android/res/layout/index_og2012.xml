<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textViewIndexOg20121"
        style="@style/CodeFontH1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:layout_marginTop="42dp"
        android:text="@string/selectSport"
        android:textSize="25dp" />
    
    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="56dp"
        android:layout_marginRight="14dp"
        android:src="@drawable/afb" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/imageButton1"
        android:layout_marginLeft="15dp"
        android:src="@drawable/vb" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_alignTop="@+id/imageButton1"
        android:layout_centerHorizontal="true"
        android:src="@drawable/tn" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_above="@+id/imageButton9"
        android:layout_alignLeft="@+id/imageButton9"
        android:src="@drawable/rb" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_above="@+id/imageButton1"
        android:layout_alignLeft="@+id/imageButton1"
        android:src="@drawable/hk" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_above="@+id/imageButton1"
        android:layout_alignLeft="@+id/imageButton8"
        android:src="@drawable/fb" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_above="@+id/imageButton5"
        android:layout_alignLeft="@+id/imageButton5"
        android:src="@drawable/sw" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_above="@+id/imageButton6"
        android:layout_alignLeft="@+id/imageButton6"
        android:src="@drawable/bk" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="@dimen/sizeButtonGeneral"
        android:layout_height="@dimen/sizeButtonGeneral"
        android:layout_above="@+id/imageButton6"
        android:layout_alignLeft="@+id/imageButton4"
        android:src="@drawable/bb" />

    <Button
        android:id="@+id/ButtonExitIndexOg20121"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_below="@+id/imageButton8"
        android:text="@string/exit"
        android:textAppearance="@style/CodeFontButton"
        android:textColor="@color/colorNo"
        android:width="100sp" />

    

</RelativeLayout>